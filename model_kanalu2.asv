function [ datasum ] = model_kanalu2( symboly1, symboly2, L1, L2, Pn )
    nuly (numel(symboly1),1) = (1+1j)*1e-6;
    sumik = awgn(nuly, 1/Pn, 1, 'linear');
    
    datasum = L1 * symboly1 + L2 * symboly2 + sumik;
    
